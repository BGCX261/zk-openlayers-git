<?page title="ZK Openlayer Demo"?>
<?script src="http://maps.google.com/maps/api/js?v=3.5&amp;sensor=false"?>
<zk>
	<hlayout vflex="1">
		<window title="Google V3"  vflex="1" border="normal" hflex="1"
			apply="ctrl.DemoWindowComposer">
			<olmaps id="myComp" vflex="1" hflex="1">
				<template name="layer">
					<ollayer vendor="Google.V3" type="google.maps.MapTypeId.TERRAIN" />
					<ollayer vendor="Google.V3" numZoomLevels="20" />
					<ollayer vendor="Google.V3" type="google.maps.MapTypeId.HYBRID"
						numZoomLevels="20" />
					<ollayer vendor="Google.V3" type="google.maps.MapTypeId.SATELLITE"
						numZoomLevels="22" />
				</template>
				<template name="control">


				</template>
				<template name="popup">


				</template>
			</olmaps>
		</window>
		<!-- <iframe src="http://localhost:8080/openlayers/google.zul"  vflex="1" hflex="1" /> -->
	</hlayout>
</zk>