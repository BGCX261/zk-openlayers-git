<?script src="http://maps.google.com/maps/api/js?v=3.5&amp;sensor=false"?>
<zk>
	<window title="Google V3" vflex="1" border="normal" hflex="1"
		apply="ctrl.DemoWindowComposer">
		<olmaps id="myComp" vflex="1" hflex="1">
			<template name="layer">
				<olgooglev3 type="google.maps.MapTypeId.TERRAIN" />
				<olgooglev3 numZoomLevels="20" />
				<olgooglev3 type="google.maps.MapTypeId.HYBRID"
					numZoomLevels="20" />
				<olgooglev3 type="google.maps.MapTypeId.SATELLITE"
					numZoomLevels="22" />
			</template>
			<template name="control">


			</template>
			<template name="popup">


			</template>
		</olmaps>
	</window>
</zk>